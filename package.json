{
  "name": "@fairfooddata/tm-editor",
  "version": "0.1.0",
  "description": "Edit NFTs for use with Trace.Market",
  "productName": "Trace.Market Editor",
  "author": "Szabolcs Szekelyi <szabi@aura-labs.cc>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "schema:merge": "node scripts/merge-types-schema.js",
    "dev:merged": "npm run schema:merge && VITE_USE_MERGED_SCHEMA=1 quasar dev",
    "build": "quasar build",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@colony/colony-js": "^8.1.0",
    "@ethersphere/bee-js": "^7.1.0",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@quasar/extras": "^1.16.4",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.2.1",
    "ethers": "^5.7.2",
    "jsonata-visual-editor": "^0.4.3",
    "mapbox-gl": "^3.17.0",
    "pinia": "^2.2.1",
    "quasar": "^2.6.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "thirdweb": "^5.44.1",
    "typia": "^6.5.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@trace.market/types": "file:../tm-types",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.39",
    "prettier": "^2.5.1",
    "ts-patch": "^3.2.1",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": "^20 || ^18 || ^16 || ^21",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "packageManager": "yarn@4.3.1"
}
